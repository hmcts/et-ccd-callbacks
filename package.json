{
    "name": "et-ccd-callbacks",
    "description": "End to End CCD tests for ET",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@hmcts/nodejs-logging": "4.0.4",
        "@wdio/cli": "9.2.1",
        "@wdio/sauce-service": "9.2.1",
        "@wdio/selenium-standalone-service": "8.15.0",
        "@wdio/types": "9.1.3",
        "axios": "1.7.8",
        "codeceptjs-saucehelper": "1.4.0",
        "cross-env": "7.0.3",
        "eslint": "9.12.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-codeceptjs": "1.3.0",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-jest": "^28.0.0",
        "eslint-plugin-mocha": "10.5.0",
        "eslint-plugin-prettier": "5.2.1",
        "html_codesniffer": "2.5.1",
        "otp": "1.1.2",
        "playwright": "1.42.1",
        "playwright-core": "1.42.1",
        "randomstring": "1.3.0",
        "request": "2.88.2",
        "request-promise-native": "1.0.9",
        "require-directory": "2.1.1",
        "totp-generator": "1.0.0"
    },
    "scripts": {
        "test:local": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run --steps -c src/test/end-to-end/codecept.conf.js --steps --reporter mocha-multi --verbose",
        "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run --steps -c src/test/end-to-end/codecept.conf.js --grep @smoke --steps --reporter mocha-multi --verbose",
        "test:fullfunctional": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run --steps -c src/test/end-to-end/codecept.conf.js --grep @nightly --steps --reporter mocha-multi --verbose",
        "test:crossbrowser": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run-multiple ${BROWSER_GROUP:-'--all'} --steps -c src/test/end-to-end/saucelabs.conf.js  --grep @crossbrowser --steps --reporter mocha-multi --verbose",
        "test:functional": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run --steps -c src/test/end-to-end/codecept.conf.js --grep @dummy --steps --reporter mocha-multi --verbose",
        "codeceptjs": "codeceptjs run --steps",
        "codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
        "tests:a11y": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=test LOG_LEVEL=ERROR mocha --exit --reporter mochawesome --require ts-node/register --require tsconfig-paths/register src/test/a11y/a11y.ts --timeout 60000 --reporter-options reportDir=functional-output/accessibility-report,reportFilename=a11y,inlineAssets=true,reportTitle=et-sya-frontend"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.17.0",
        "@codeceptjs/configure": "1.0.1",
        "@codeceptjs/examples": "1.2.4",
        "@codeceptjs/ui": "1.2.0",
        "@types/archiver": "^6.0.0",
        "@types/chai-as-promised": "^8.0.0",
        "@types/fs-extra": "^11.0.0",
        "@types/git-rev-sync": "2.0.2",
        "@types/jest": "^29.0.0",
        "@types/lodash.clonedeep": "^4.5.7",
        "@types/lodash.isobject": "^3.0.7",
        "@types/lodash.isplainobject": "^4.0.7",
        "@types/lodash.zip": "^4.2.7",
        "@types/pino-std-serializers": "^4.0.0",
        "@types/redis-mock": "^0.17.1",
        "@types/sinon-chai": "^4.0.0",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "@wdio/cli": "^9.0.0",
        "@wdio/sauce-service": "^9.0.0",
        "@wdio/selenium-standalone-service": "^8.0.0",
        "@wdio/types": "^9.0.0",
        "chai": "4.5.0",
        "chai-as-promised": "8.0.1",
        "chai-http": "5.1.1",
        "codeceptjs": "3.6.7",
        "codeceptjs-saucehelper": "^1.4.0",
        "eslint": "9.12.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-codeceptjs": "^1.3.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-mocha": "^10.1.0",
        "eslint-plugin-prettier": "^5.0.0",
        "husky": "9.1.7",
        "istanbul": "1.1.0-alpha.1",
        "mocha": "10.7.3",
        "mocha-jenkins-reporter": "0.4.8",
        "mocha-junit-reporter": "2.2.1",
        "mocha-multi": "1.1.7",
        "mochawesome": "7.1.3",
        "nock": "13.5.6",
        "nodemon": "3.1.7",
        "nsp": "3.2.1",
        "pa11y": "8.0.0",
        "playwright": "^1.35.1",
        "playwright-core": "^1.35.1",
        "proxyquire": "2.1.3",
        "puppeteer": "23.5.3",
        "rewire": "7.0.0",
        "selenium-standalone": "10.0.0",
        "sinon": "19.0.2",
        "sinon-chai": "4.0.0",
        "sonar-scanner": "3.1.0",
        "sonarqube-scanner": "4.2.5",
        "superagent-proxy": "3.0.0",
        "supertest": "7.0.0",
